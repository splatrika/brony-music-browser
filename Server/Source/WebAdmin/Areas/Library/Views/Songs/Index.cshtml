@model LibraryIndexViewModel<Song>

@{
    ViewData["CurrentResource"] = "Songs";
}

@{
    var indexModel = new IndexPartialViewModel
    {
        Page = Model.Page,
        PageStep = Model.PageStep,
        Items = Model.Items,
        ActionsPartialName = "_UpdateRemoveActionsPartial",
        Columns = new[]
        {
            new IndexPartialViewModel.ColumnInfo
            {
                Name = "Id",
                Getter = x => ((Song)x).Id
            },
            new IndexPartialViewModel.ColumnInfo
            {
                Name = "Title",
                Getter = x => ((Song)x).Title
            }
        }
    };
}

<partial name="_IndexPartial" model="indexModel" />
